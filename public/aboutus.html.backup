    <link rel="stylesheet" type="text/css" href="./aboutus_files/idangerous.swiper.min.css" />
    <script src="./aboutus_files/idangerous.swiper.min.js"></script>
    <script type="text/javascript">
      var offsetHeight = 0
      var opinionBreakHeight = 0
      var businessBreakHeight = 0
      var layoutBreakHeight = 0
      var irBreakHeight = 0
      var newsBreakHeight = 0
      var awardBreakHeight = 0
      var responsibilityBreakHeight = 0

      var hasBusinessAniExe = false
      var hasResponsibilityAniExe = false

      var bannerSwiper
      $(function () {
        if ($('.banner .swiper-container').length > 0) {
          initBannerFunc()
        }

        initBusinessFunc()
        initLayoutFunc()
        initIrFunc()
        initResponsibilityFunc()
        initAwardFunc()

        //加载动画的高度

        initSectionSize()

        $(window).resize(
          debounce(function () {
            bannerSwiper.reInit()
            responsibilitySwiper.reInit()
            awardSwiper.reInit()

            initSectionSize()
          }, 10)
        )

        setSectionClass(0)

        $(window).scroll(function () {
          var scrollTop = $(window).scrollTop()

          setLayoutParallax(scrollTop)

          setSectionClass(scrollTop)
        })
      })

      function initSectionSize() {
        offsetHeight = $(window).height() / 3
        opinionBreakHeight = $('section.opinion').offset().top - $(window).height() + offsetHeight
        businessBreakHeight = $('section.business').offset().top - $(window).height() + offsetHeight
        layoutBreakHeight = $('section.layout').offset().top - $(window).height() + offsetHeight
        irBreakHeight = $('section.ir').offset().top - $(window).height() + offsetHeight
        newsBreakHeight = $('section.news').offset().top - $(window).height() + offsetHeight
        awardBreakHeight = $('section.award').offset().top - $(window).height() + offsetHeight
        //responsibilityBreakHeight = $('section.responsibility').offset().top - $(window).height() + offsetHeight;
      }

      function setSectionClass(scrollTop) {
        if (scrollTop > opinionBreakHeight) {
          $('section.opinion').addClass('active')
        }

        if (scrollTop > businessBreakHeight) {
          if (!hasBusinessAniExe) {
            $('section.business').addClass('active')
            setTimeout(function () {
              businessAni(0)
            }, 1200)
            hasBusinessAniExe = true
          }
        }

        if (scrollTop > layoutBreakHeight) {
          $('section.layout').addClass('active')
        }

        if (scrollTop > irBreakHeight) {
          $('section.ir').addClass('active')
        }

        if (scrollTop > newsBreakHeight) {
          $('section.news').addClass('active')
        }
        if (scrollTop > awardBreakHeight) {
          $('section.award').addClass('active')
        }

        /*if(scrollTop > responsibilityBreakHeight){
          if(!hasResponsibilityAniExe){
              responsibilitySwiper.swipeNext();
              hasResponsibilityAniExe = true;
          }
 
      }*/
      }

      function initBannerFunc() {
        // bannerSwiper.stopAutoplay();
        // bannerSwiper.swipeTo(0);

        $('.banner .pagination span').click(function () {
          bannerSwiper.swipeTo($(this).index())
        })

        if (!IsPC()) {
          bannerSwiper = new Swiper('.banner .swiper-container', {
            autoplay: 9000,
            loop: true,
            pagination: '.banner .pagination',
            autoplayDisableOnInteraction: false,
          })
          bannerSwiper.stopAutoplay()
          $('.banner .video-box').remove()
        }

        var $bannerVideo = $('.banner .video-box video')
        if ($bannerVideo.length) {
          $bannerVideo[0].onended = function () {
            $('.banner .video').remove()
            $('.banner .inner').addClass('hide')
            bannerSwiper = new Swiper('.banner .swiper-container', {
              autoplay: 9000,
              loop: true,
              pagination: '.banner .pagination',
              autoplayDisableOnInteraction: false,
            })
            bannerSwiper.startAutoplay()
            bannerSwiper.swipeTo(0)
            setTimeout(function () {
              $('.banner .video-box').remove()
            }, 3000)
          }
        }
      }

      function initBusinessFunc() {
        $('.business .list li').each(function (index) {
          $(this).css({
            'transition-delay': index * 0.1 + 's',
            '-webkit-transition-delay': index * 0.1 + 's',
          })
        })
        $('.business .list li').hover(function () {
          businessAni($(this).index())
        })
      }

      function businessAni(index) {
        $('.business .detail>div').css('display', 'none').removeClass('active')
        $('.business .list li').removeClass('hover')
        //set active
        $('.business .list li').eq(index).addClass('hover')
        var $detail = $('.business .detail>div').eq(index)
        $detail.css('display', 'block')
        setTimeout(function () {
          $detail.addClass('active')
        }, 20)
      }

      var $layout
      var winHeight
      var moveRate
      var layoutOffsetTop
      var layoutHeight

      function initLayoutFunc() {
        var $layout = $('.layout')
        var $bg = $('.layout .bg>div')
        var $link = $('.layout .list a')
        var index = 0
        $bg.eq(index).addClass('active')
        $link.eq(index).addClass('hover')

        $link.hover(function () {
          var indexNow = $(this).parent().index()
          if (index != indexNow) {
            index = indexNow
            $bg.removeClass('active')
            $link.removeClass('hover')
            $bg.eq(index).addClass('active')
            $link.eq(index).addClass('hover')
          }
        })

        winHeight = $(window).height()
        layoutHeight = $layout.outerHeight(true)
        layoutOffsetTop = $layout.offset().top
        moveRate = ($('.layout .bg>div').outerHeight(true) - layoutHeight - 50) / (winHeight + layoutHeight)
      }

      function setLayoutParallax(scrollTop) {
        if (scrollTop >= layoutOffsetTop - winHeight && scrollTop <= layoutOffsetTop + layoutHeight) {
          var parallax = scrollTop - (layoutOffsetTop - winHeight)
          var translateY = -moveRate * parallax

          translateY = translateY / 2

          $('.layout .bg').css({
            '-webkit-transform': 'translateY(' + translateY + 'px)',
            '-ms-transform': 'translateY(' + translateY + 'px)',
            transform: 'translateY(' + translateY + 'px)',
          })
        }
      }

      function initIrFunc() {
        $('.ir .item .text-ellipsis').each(function (index) {
          $(this).css({
            'transition-delay': index * 0.2 + 's',
            '-webkit-transition-delay': index * 0.2 + 's',
          })
        })
      }

      var responsibilitySwiper
      var swiperLength = 0

      function initResponsibilityFunc() {
        if (!IsPC()) {
          $('.responsibility .swiper-slide video').remove()
        }

        swiperLength = $('.responsibility .swiper-slide').length

        responsibilitySwiper = new Swiper('.responsibility .swiper-container', {
          onSlideChangeStart: function (swiper) {
            setSwiperArrowVisible(swiper.activeIndex)

            var $video = $('.responsibility .swiper-slide').eq(swiper.activeIndex).find('video').length
            if ($video.length > 0) {
              $video[0].currentTime = 0
            }
          },
        })

        setSwiperArrowVisible(0)

        $('.responsibility .arrow.prev').click(function () {
          responsibilitySwiper.swipePrev()
        })
        $('.responsibility .arrow.next').click(function () {
          responsibilitySwiper.swipeNext()
        })
      }

      function setSwiperArrowVisible(index) {
        $('.responsibility .arrow').show()
        if (index == 0) {
          $('.responsibility .arrow.prev').hide()
        }
        if (index == swiperLength - 1) {
          $('.responsibility .arrow.next').hide()
        }
      }

      var awardSwiper

      function initAwardFunc() {
        awardSwiper = new Swiper('.award .swiper-container', {
          autoplay: 3000,
          loop: true,
          pagination: '.award .pagination',
          autoplayDisableOnInteraction: false,
        })
        $('.award .pagination span').click(function () {
          awardSwiper.swipeTo($(this).index())
        })
      }
    </script>

